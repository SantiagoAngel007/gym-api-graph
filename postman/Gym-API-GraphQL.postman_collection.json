      "name": "Authentication",
      "item": [
        {
          "name": "Signup - Create User",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "  try {",
                  "    const jsonData = pm.response.json();",
                  "    if (jsonData.data && jsonData.data.signup) {",
                  "      const { token, user } = jsonData.data.signup;",
                  "      pm.environment.set('AUTH_TOKEN', token);",
                  "      pm.environment.set('USER_ID', user.id);",
                  "      console.log('✓ TOKEN guardado:', token.substring(0, 20) + '...');",
                  "      console.log('✓ USER_ID guardado:', user.id);",
                  "    }",
                  "  } catch (e) {",
                  "    console.log('Error al guardar variables:', e);",
                  "  }",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"}
            ],
            "body": {
              "mode": "graphql",
              "graphql": {
                "query": "mutation signup($createUserInput: CreateUserInput!) {\n  signup(createUserInput: $createUserInput) {\n    token\n    user {\n      id\n      email\n      fullName\n      age\n      isActive\n    }\n  }\n}"
              }
            },
            "url": {"raw": "{{BASE_URL}}/graphql", "protocol": "http", "host": ["{{BASE_URL_HOST}}"], "port": "{{BASE_URL_PORT}}", "path": ["graphql"]}
          }
